#tuple_unpacking
for (a,b) in mylist:
    print(a)
    print(b)

mylist10 = [(1,2,3),(5,6,7),(8,9,10)]
for a,b,c in mylist10:
    print(a)
    print(b)
    print(c)

d = {'k1':1,'k2':2 ,'k3':3}
for item in d.items():
    print(item)
    
d = {'k1':1,'k2':2 ,'k3':3}
for key,value in d.items():
    print(value)
    
d = {'k1':1,'k2':2 ,'k3':3}
for value in d.values():
    print(value)    

