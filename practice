mystring = "wipro"
for letter in mystring:
    if letter=='p':
        continue
    print(letter)
    
mystring = 'wipropython'
for letter in mystring:
    if letter=='p':
        break
    print(letter)
    
for num in range(0,10,2):
    print(num)
    
print(list (range (0,11,2)))
print('\n')

index=0
word ='abcde'
for letter in word:
    print(word[index])
    index+=1
print('\n')
#enrumate
word = 'abcde'
for index,letter in enumerate(word):
    print(index)
    print(letter)
    print('\n')
#zipping pair up ;P
myli=[1,2,3]
myli2=[a,b,c]
myli3=[100,900,800]
for item in zip(myli,myli2,myli3):
    print(item)
print('\n')
print(list(zip(myli,myli2,myli3)))       
